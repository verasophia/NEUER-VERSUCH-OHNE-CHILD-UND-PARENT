@Article{Sala2016,
  author    = {Giovanni Sala and Fernand Gobet},
  journal   = {Educational Research Review},
  title     = {When the music{\textquotesingle}s over. Does music skill transfer to children{\textquotesingle}s and young adolescents{\textquotesingle} cognitive and academic skills? A meta-analysis},
  year      = {2016},
  month     = {feb},
  pages     = {55--67},
  volume    = {20},
  abstract  = {Music training has been recently claimed to enhance children and young adolescents' cognitive and academic skills. However, substantive research on transfer of skills suggests that far-transfer - i.e., the transfer of skills between two areas only loosely related to each other - occurs rarely. In this meta-analysis, we examined the available experimental evidence regarding the impact of music training on children and young adolescents' cognitive and academic skills. The results of the random-effects models showed (a) a small overall effect size ; (b) slightly greater effect sizes with regard to intelligence and memory-related outcomes ; and (c) an inverse relation between the size of the effects and the methodological quality of the study design. These results suggest that music training does not reliably enhance children and young adolescents' cognitive or academic skills, and that previous positive findings were probably due to confounding variables.},
  doi       = {10.1016/j.edurev.2016.11.005},
  groups    = {Music},
  keywords  = {Music training Transfer Cognitive skills Education Meta-analysis},
  publisher = {Elsevier {BV}},
}

